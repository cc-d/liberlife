{
  "configurations": [
    {
      "type": "python",
      "request": "launch",
      "name": "Run Tests",
      "module": "pytest",
      "env": {
        "LOGF_USE_PRINT": "true",
      },
      "args": [
        "tests",
        "-s",
        "--show-capture=all",
        "-x",
        "--cov",
        "--cov-report=term-missing",
      ],
      "cwd": "${workspaceFolder}/api",
    }

  ]
}
